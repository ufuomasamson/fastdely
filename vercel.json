{
  "version": 2,
  "builds": [
    {
      "src": "client/site/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "client/admin/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/login.html",
      "dest": "/client/admin/login.html"
    },
    {
      "src": "/login",
      "dest": "/client/admin/login.html"
    },
    {
      "src": "/tracking",
      "dest": "/client/site/tracking-result.html"
    },
    {
      "src": "/admin",
      "dest": "/client/admin/login.html"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/client/admin/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/site/$1"
    }
  ]
} 