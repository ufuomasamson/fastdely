{
  "version": 2,
  "framework": null,
  "buildCommand": null,
  "devCommand": null,
  "installCommand": null,
  "builds": [
    {
      "src": "site/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "admin/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/login.html",
      "dest": "/admin/login.html"
    },
    {
      "src": "/login",
      "dest": "/admin/login.html"
    },
    {
      "src": "/tracking",
      "dest": "/site/tracking-result.html"
    },
    {
      "src": "/admin",
      "dest": "/admin/login.html"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/admin/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/site/$1"
    }
  ]
} 